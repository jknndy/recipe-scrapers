        <script type="text/javascript">
          setTimeout(() => {
            if (document.cookie.split(';').some(item => item.includes('sp_rewe=1'))) {
              let reweButtonScript = document.createElement('script');
              reweButtonScript.setAttribute('src', 'https://shop.rewe.de/ingredients/button.js');
              document.body.appendChild(reweButtonScript);
            } else {
              document.getElementById('rewe-button').style.display = 'none';
              document.getElementById('rewe-button-placeholder').style.display = 'block';
            }
          }, 10);

          (function () {
            document.querySelectorAll('.js-open-backdrop-overlay').forEach((backdropOverlay) => {
              backdropOverlay.addEventListener('click', () => {
                openBackdropOverlay(backdropOverlay.dataset.backdropOverlayId)
              })
            })

            document.querySelectorAll('.js-close-backdrop-overlay').forEach((backdropOverlay) => {
              backdropOverlay.addEventListener('click', () => {
                closeBackdropOverlay(backdropOverlay.dataset.backdropOverlayId)
              })
            })

            document.addEventListener('ckOpenBeteRDSInfoBoxOnce', (event) => {
              openBackdropOverlay(event.detail.id)
            }, { once: true })

            function openBackdropOverlay(id) {
              const target = document.getElementById(id);
              if (null === target) return;

              target.classList.remove('display-none');
              document.body.addEventListener('keydown', function (e) {
                if (e.key === 'Escape') {
                  closeBackdropOverlay(id);
                }
              });
            }

            function closeBackdropOverlay(id) {
              const target = document.getElementById(id);

              if (null === target) return;

              target.classList.add('display-none');
            }
          })();
        </script>
    
